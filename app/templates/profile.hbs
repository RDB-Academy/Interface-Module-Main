{{outlet}}
<div class="container">
  <div class="fb-profile">
      <img align="left" class="fb-image-lg" src="/img/1-0.png"/>
      <img align="left" class="fb-image-profile thumbnail img-circle" src="img/alchemist_logo.jpg"/>
      <div class="page-header" style="margin-bottom: 4em;">
        <h1>{username} <small>{current status} </small></h1>
      </div>
  </div>
  <div class="feature-list">
    <div class="panel panel-default">
      <div class="panel-heading">
        your stats
      </div>
      <div class="row">
        <div class="col-sm-5 col-xs-10 col-sm-push-7 col-xs-push-1">
          {{profile-chart}}
        </div>
        <div class="col-sm-6 col-xs-12 col-sm-pull-4">
          <table class="table table-striped table-hover">
            <tbody>
            {{#each array as |x|}}
              <tr>
                <td>bla</td>
                <td>asd</td>
              </tr>
            {{/each}}
            </tbody>
          </table>
        </div>

      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        Recent Achievements
      </div>
      <div class="row">
          {{#each array as |x|}}
              <div class="col-md-1 col-sm-2 col-xs-3">
                  <div class="thumbnail">
                      <img class="thumbnail-object" src="img/gamification-word-cloud.jpg"/>
                  </div>
              </div>
          {{/each}}
      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        some fancy carousel
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-3"/>
          <div class="col-md-4">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
              <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>
              </ol>
              <div class="carousel-inner" role="listbox">
                <div class="item active">
                  <div class="thumbnail">
                    <img class="thumbnail-object" src="img/gamification-word-cloud.jpg"/>
                  </div>
                </div>
                <div class="item">
                  <div class="thumbnail">
                    <img class="thumbnail-object" src="img/gamification-word-cloud.jpg"/>
                  </div>
                </div>
                <div class="item">
                  <div class="thumbnail">
                    <img class="thumbnail-object" src="img/gamification-word-cloud.jpg"/>
                  </div>
                </div>
                <div class="item">
                  <div class="thumbnail">
                    <img class="thumbnail-object" src="img/gamification-word-cloud.jpg"/>
                  </div>
                </div>
              </div>
              <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                  <span class="glyphicon glyphicon-right" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="panel panel-default">

      <div class="panel-heading">
        some fancy chart
      </div>
      {{profile-chart-bottom}}
    </div>
  </div> <!-- /.Feature-List -->
</div>
