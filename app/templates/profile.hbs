{{outlet}}
<div class="container">
  <div class="row">
    <div class="col-sm-3 col-xs-5">
      <img align="left" class="fb-image-profile thumbnail img-circle" src="img/alchemist_logo.jpg"/>
    </div>
    <div class="col-xs-7 col-sm-9">
      <h1>{username}<br class="visible-xs"><small> {current status} </small></h1><hr>
    </div>
    <div class="col-xs-7 col-sm-9">
      <div class="row">
        <div class="col-sm-7">
          <table class="table table-striped table-hover profile-stats">
            <tbody>
            {{#each array as |x|}}
              <tr>
                <td>bla</td>
                <td>asd</td>
              </tr>
            {{/each}}
            </tbody>
          </table>
        </div>
        <div class="col-sm-5">
          {{profile-chart}}
        </div>
      </div>
    </div>
  </div>
  <div class="feature-list">
    <div class="panel panel-default">
      <div class="panel-heading">
        Recent Achievements
      </div>
      <div class="row">
          {{#each array as |x|}}
              <div class="col-md-1 col-sm-2 col-xs-3">
                  <div class="thumbnail">
                      <img class="thumbnail-object" src="img/gamification-word-cloud.jpg"/>
                  </div>
              </div>
          {{/each}}
      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        some fancy carousel
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-8 vertical-center">
            {{profile-chart-bottom}}
          </div><!--
       --><div class="col-md-4 vertical-center">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
              <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>
              </ol>
              <div class="carousel-inner" role="listbox">
                <div class="item active">
                  <div class="panel panel-default">
                    <div class="panel-heading">
                      Relational Algebra
                    </div>
                    <div class="panel-body">
                      <table class="table table-striped table-hover">
                        <tbody>
                        {{#each array as |x|}}
                          <tr>
                            <td>bla</td>
                            <td>asd</td>
                          </tr>
                        {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
                <div class="item">
                  <div class="panel panel-default">
                    <div class="panel-heading">
                      Relational Algebra
                    </div>
                    <div class="panel-body">
                      <table class="table table-striped table-hover">
                        <tbody>
                        {{#each array as |x|}}
                          <tr>
                            <td>bla</td>
                            <td>asd</td>
                          </tr>
                        {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
                <div class="item">
                  <div class="panel panel-default">
                    <div class="panel-heading">
                      Relational Algebra
                    </div>
                    <div class="panel-body">
                      <table class="table table-striped table-hover">
                        <tbody>
                        {{#each array as |x|}}
                          <tr>
                            <td>bla</td>
                            <td>asd</td>
                          </tr>
                        {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
                <div class="item">
                  <div class="panel panel-default">
                    <div class="panel-heading">
                      Relational Algebra
                    </div>
                    <div class="panel-body">
                      <table class="table table-striped table-hover">
                        <tbody>
                        {{#each array as |x|}}
                          <tr>
                            <td>bla</td>
                            <td>asd</td>
                          </tr>
                        {{/each}}
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
              <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev" style="background:transparent">
                <span class="glyphicon glyphicon-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
              </a>
              <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next" style="background:transparent">
                  <span class="glyphicon glyphicon-right"></span>
                  <span class="sr-only">Next</span>
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div> <!-- /.Feature-List -->
</div>
