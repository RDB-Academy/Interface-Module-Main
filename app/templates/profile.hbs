{{outlet}}
<div class="container">
  <div class="row">
    <div class="col-sm-4 hidden-xs" style="height:100%">
      <img class="fb-image-profile thumbnail img-circle" src="img/alchemist_logo.jpg"/>
    </div>
    <div class="col-xs-8 col-sm-8"> <!-- max username: 14 -->
      <h1>{usernameasdasd}<br class="visible-xs"><small> {current status} </small></h1><hr>
    </div>
    <div class="col-xs-4 visible-xs">
      <img align="right" class="fb-image-profile thumbnail img-circle" src="img/alchemist_logo.jpg"/>
    </div>
    <div class="col-xs-12 col-sm-4">
      <table class="table table-striped table-hover profile-stats">
        <tbody>
          <tr>
            <td>Last Login</td><td> {{model.profileStats.a}} </td>
          </tr>
          <tr>
            <td>Total Time Spend</td><td> {{model.profileStats.b}} </td>
          </tr>
          <tr>
            <td>Total Tasks Done</td><td> {{model.profileStats.c}} </td>
          </tr>
          <tr>
            <td>Task Points</td><td> {{model.profileStats.d}} </td>
          </tr>
          <tr>
            <td>Achievement Points</td><td> {{model.profileStats.e}} </td>
          </tr>
          <tr>
            <td>Most played Mode</td><td> {{model.profileStats.f}} </td>
          </tr>
          <tr>
            <td>Total Points</td><td> {{model.profileStats.g}} </td>
          </tr>
        </tbody>
      </table>
    </div>
    <div class="col-sm-4 hidden-xs">
      {{profile-chart}}
    </div>
    <div class="clearfix"/>
    <div class="col-xs-12 col-md-12">
      <div class="col-md-8 hidden-xs" style="position:absolute;top:-90px">
        <h3>Recent Achievements</h3>
        <hr>
      </div>
    </div>
  </div>




  <div class="feature-list">
    <div class="visible-xs">
      <h3>Recent Achievements</h3>
      <hr>
    </div>
    <div class="well">
      <div class="row">
        {{#each model.achievements as |achievement|}}
            <div class="col-sm-1 col-xs-3">
                {{achievement-small achievement}}
            </div>
        {{/each}}
      </div>
    </div>


    <div class="panel panel-default hidden-xs">
      <div class="panel-heading">
        some fancy carousel
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-8 vertical-center">
            {{profile-chart-bottom model.chartData}}
          </div><!--
       --><div class="col-md-4 vertical-center">
            {{profile-carousel model.chartData}}
          </div>
        </div>
      </div>
    </div>

    <div id="myCarousel2" class="carousel slide hidden-md hidden-lg hidden-sm" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#myCarousel2" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel2" data-slide-to="1"></li>
        <li data-target="#myCarousel2" data-slide-to="2"></li>
        <li data-target="#myCarousel2" data-slide-to="3"></li>
      </ol>
      <div class="carousel-inner" role="listbox">
        <div class="item active">
          <div class="panel panel-default">
            <div class="panel-heading">
              Relational Algebra
            </div>
            <div class="panel-body">
              <table class="table table-striped table-hover">
                <tbody>
                {{#each array as |x|}}
                  <tr>
                    <td>bla</td>
                    <td>asd</td>
                  </tr>
                {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class="item">
          <div class="panel panel-default">
            <div class="panel-heading">
              Relational Algebra
            </div>
            <div class="panel-body">
              <table class="table table-striped table-hover">
                <tbody>
                {{#each array as |x|}}
                  <tr>
                    <td>bla</td>
                    <td>asd</td>
                  </tr>
                {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class="item">
          <div class="panel panel-default">
            <div class="panel-heading">
              Relational Algebra
            </div>
            <div class="panel-body">
              <table class="table table-striped table-hover">
                <tbody>
                {{#each array as |x|}}
                  <tr>
                    <td>bla</td>
                    <td>asd</td>
                  </tr>
                {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div class="item">
          <div class="panel panel-default">
            <div class="panel-heading">
              Relational Algebra
            </div>
            <div class="panel-body">
              <table class="table table-striped table-hover">
                <tbody>
                {{#each array as |x|}}
                  <tr>
                    <td>bla</td>
                    <td>asd</td>
                  </tr>
                {{/each}}
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
      <a class="left carousel-control" href="#myCarousel2" role="button" data-slide="prev" style="background:transparent">
        <span class="glyphicon glyphicon-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel2" role="button" data-slide="next" style="background:transparent">
          <span class="glyphicon glyphicon-right"></span>
          <span class="sr-only">Next</span>
      </a>
    </div>


  </div> <!-- /.Feature-List -->
</div>
