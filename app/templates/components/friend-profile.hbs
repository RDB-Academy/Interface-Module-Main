<li class="list-group-item">
  <div class="col-xs-2 image">
    <img align=left class="fb-image-profile thumbnail img-circle" src="{{friend-profile.image}}"/>
  </div>
  <div class="col-xs-8 user-information">
    {{friend-profile.username}}
  </div>
  <div class="col-xs-2">
    {{yield}}
  </div>
  <div class="col-xs-6">
    Title: {{friend-profile.title}}
  </div>
  <div class="col-xs-6">
    Total Score: {{friend-profile.points}}
  </div>
  <div class="clearfix"></div>
</li>
