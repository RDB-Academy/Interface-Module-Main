{{outlet}}
<div class="container">
	<div class="row">
    <div class="col-sm-4 col-sm-push-8">
      <!-- Placeholder Searchbar! -->
      <form action="#" method="get">
        <div class="input-group">
          <input class="form-control" id="system-search" name="q" placeholder="Search User..." required>
          <span class="input-group-btn">
            <button type="submit" class="btn btn-default"><i class="glyphicon glyphicon-search"></i></button>
          </span>
        </div>
      </form>
      <br>
      <h1>Friend Requests <br><small>want to be friends?</small></h1>
			<ul class="friend-request">
				{{#each model.a as |profile|}}
					{{#friend-request profile}}
				  {{/friend-request}}
				{{/each}}
			</ul>
    </div>
    <div class="col-sm-8 col-sm-pull-4">
      <h1>Friends</h1>
			<ul>
				{{#each model.b as |profile|}}
					{{#friend-profile profile}}
				  {{/friend-profile}}
				{{/each}}
			</ul>
    </div>
	</div>
</div>
